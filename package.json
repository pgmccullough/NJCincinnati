{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "create_branch": "func() { git checkout -b $1 && git push -u origin $1; }; func",
    "delete_branch": "func() { git checkout main && git branch -d $1 && git push origin -d $1; }; func",
    "dev": "remix build && run-p \"dev:*\"",
    "dev:node": "cross-env NODE_ENV=development nodemon --require dotenv/config ./server.js --watch ./server.js",
    "dev:remix": "remix watch",
    "sass": "sass --watch app/:app/",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@lexical/react": "^0.10.0",
    "@prisma/client": "^4.13.0",
    "@remix-run/css-bundle": "^1.16.0",
    "@remix-run/express": "^1.16.0",
    "@remix-run/node": "^1.16.0",
    "@remix-run/react": "^1.16.0",
    "@types/bcrypt": "^5.0.0",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "isbot": "^3.6.8",
    "lexical": "^0.10.0",
    "morgan": "^1.10.0",
    "prisma": "^4.13.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.16.0",
    "@remix-run/eslint-config": "^1.16.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "dotenv": "^16.0.3",
    "eslint": "^8.38.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "sass": "^1.62.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=14"
  }
}
